<div class="page-header">
	<h1>All projects </h1>
</div>

<table class="table table-striped table-bordered table-condensed">
	<thead>
		<tr>
      <th>Title</th>
      <th>Description</th>
      <th>Manager</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
	</thead>
		
	<tbody>
		<% @projects.each do |project| %>
			<tr>
				<td><%= link_to "#{project.name}", project_path(project) %></td>
				<td><%= link_to "#{project.description}", project_path(project) %></td>
				<td> Foobar </td>
				<td>Edit</td>
				<td><%= link_to "Delete", project_path(project), :method => "delete", :confirm => "Are you sure?" %></td>
			</tr>	
		<% end %>
	</tbody>

</table>

<%= link_to "Add objective to project", new_objective_path, :class => "btn", :type => "button" %>