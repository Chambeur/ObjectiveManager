<div class="page-header">
	<h1><%= @objective.title %> </h1>
</div>

<p class="lead"> <%= @objective.description %> </p>



<%= link_to "Finish objective", root_path, :confirm => "Are you sure?", :class => "btn btn-success" %>

<%= link_to "Report objective", root_path, :confirm => "Are you sure?", :class => "btn btn-warning" %>

<%= link_to "Delete objective", objective_path(@objective), :method => "delete", :confirm => "Are you sure?", :class => "btn btn-danger" %>