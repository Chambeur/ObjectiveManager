<div class="page-header">
	<h1>All labels </h1>
</div>

<table class="table">
	<tbody>
		<% @labels.each do |label| %>
			<tr>
				<td><%= label.name, :class => "label" %> </td>
				<td><%= link_to "Edit label", label_path(label) %></td>
				<td><%= link_to "Delete", label_path(label), :method => "delete", :confirm => "Are you sure?" %></td>
			</tr>	
		<% end %>
	</tbody>

</table>

<%= link_to "Add objective to project", new_objective_path, :class => "btn", :type => "button" %>